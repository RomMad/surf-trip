<twig:Table>
    <twig:Table:Header>
        <twig:Table:Row>
            {% for header in headers %}
                <twig:Table:Head>
                    {% if header is iterable %}
                        <twig:Table:SortableHeadLink label="{{ header.0 }}" name="{{ header.1 }}" />
                    {% else %}
                        {{- header -}}
                    {% endif %}
                </twig:Table:Head>
            {% endfor %}
        </twig:Table:Row>
    </twig:Table:Header>
    <twig:Table:Body>
        {% for row in data %}
            <twig:Table:Row>
                {% for cell in row %}
                    <twig:Table:Cell>
                        {% if cell?.raw is defined %}
                            {{- cell.raw|raw -}}
                        {% elseif cell is iterable %}
                            {{- cell|join(', ') -}}
                        {% else %}
                            {{- cell -}}
                        {% endif %}
                    </twig:Table:Cell>
                {% endfor %}
            </twig:Table:Row>
        {% else %}
            <twig:Table:Row>
                <twig:Table:Cell :colspan="headers|length">
                    {{- empty_message|trans -}}
                </twig:Table:Cell>
            </twig:Table:Row>
        {% endfor %}
    </twig:Table:Body>
</twig:Table>
