<twig:Table>
    <twig:Table:Body>
        <twig:Table:Row>
            <twig:Table:Head>
                {{- 'title.label'|trans -}}
            </twig:Table:Head>
            <twig:Table:Cell>
                {{- trip.title -}}
            </twig:Table:Cell>
        </twig:Table:Row>
        <twig:Table:Row>
            <twig:Table:Head>
                {{- 'location.label'|trans -}}
            </twig:Table:Head>
            <twig:Table:Cell>
                {{- trip.location -}}
            </twig:Table:Cell>
        </twig:Table:Row>
        <twig:Table:Row>
            <twig:Table:Head>
                {{- 'start_at.label'|trans -}}
            </twig:Table:Head>
            <twig:Table:Cell>
                {{- trip.startAt|date('Y-m-d H:i') -}}
            </twig:Table:Cell>
        </twig:Table:Row>
        <twig:Table:Row>
            <twig:Table:Head>
                {{- 'end_at.label'|trans -}}
            </twig:Table:Head>
            <twig:Table:Cell>
                {{- trip.endAt|date('Y-m-d H:i') -}}
            </twig:Table:Cell>
        </twig:Table:Row>
        <twig:Table:Row>
            <twig:Table:Head>
                {{- 'required_levels.label'|trans -}}
            </twig:Table:Head>
            <twig:Table:Cell>
                {{- trip.requiredLevels|map(rl => rl.label|trans)|join(', ') -}}
            </twig:Table:Cell>
        </twig:Table:Row>
        <twig:Table:Row>
            <twig:Table:Head>
                {{- 'description.label'|trans -}}
            </twig:Table:Head>
            <twig:Table:Cell>
                {{- trip.description -}}
            </twig:Table:Cell>
        </twig:Table:Row>
        <twig:Table:Row>
            <twig:Table:Head>
                {{- 'owners.label'|trans -}}
            </twig:Table:Head>
            <twig:Table:Cell>
                {{- trip.ownerNames -}}
            </twig:Table:Cell>
        </twig:Table:Row>
        <twig:Table:Row>
            <twig:Table:Head>
                {{- 'created_at.label'|trans -}}
            </twig:Table:Head>
            <twig:Table:Cell>
                {{- trip.createdAt|date('Y-m-d H:i') -}}
            </twig:Table:Cell>
        </twig:Table:Row>
    </twig:Table:Body>
</twig:Table>
